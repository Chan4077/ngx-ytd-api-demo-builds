{
	"hosting": {
		"public": "dist/ngx-ytd-api-demo",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/latest/**/!(*.@(js|html|css|json|svg|png|jpg|jpeg))",
				"destination": "/latest/index.html"
			},
			{
				"source": "/master/**/!(*.@(js|html|css|json|svg|png|jpg|jpeg))",
				"destination": "/master/index.html"
			}
		],
		"redirects": [
			{
				"source": "/",
				"destination": "/latest",
				"type": 302
			},
			{
				"source": "/head",
				"destination": "/master",
				"type": 302
			}
		]
	}
}